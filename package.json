{
  "name": "utils-pack",
  "version": "0.0.8",
  "description": "JS Helper Functions",
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/ecoinomist/utils-pack",
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "helper",
    "utils",
    "functions"
  ],
  "scripts": {
    "push": "yarn delete src/package.json && cp {README.md,LICENSE,package.json} src/ && cd src && yarn publish",
    "build": "webpack src/index.js -o dist/index.js",
    "test": "jest",
    "delete": "./node_modules/rimraf/bin.js",
    "clear": "yarn delete yarn.lock node_modules",
    "reset": "yarn clear && yarn cache clean && yarn",
    "scan": "./node_modules/.bin/madge --circular src --exclude 'node_modules/'"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "circular-json": "^0.5.9",
    "dot-prop-immutable": "^1.5.0",
    "flat": "^4.1.0",
    "humanize-duration": "^3.15.3",
    "lodash": "^4.17.11",
    "minimist": "^1.2.3",
    "moment": "^2.22.2",
    "papaparse": "^5.2.0",
    "performance-now": "^2.1.0",
    "pluralize": "^8.0.0",
    "reselect": "^4.0.0",
    "thenby": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/node": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "babel-jest": "24.9.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "jest": "24.9.0",
    "madge": "^3.4.4",
    "rimraf": "^2.6.2"
  }
}
