{
  "name": "utils-pack",
  "version": "0.1.2",
  "description": "JS Helper Functions",
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/ecoinomist/utils-pack",
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "helper",
    "utils",
    "functions"
  ],
  "scripts": {
    "push": "yarn prebuild && yarn build && yarn dist",
    "prebuild": "yarn symlink ../babel.config.js ./",
    "build": "yarn delete dist && babel src --out-dir dist",
    "dist": "cp {README.md,LICENSE,package.json} dist/ && cd dist && yarn publish && cd ../ && yarn delete dist",
    "delete": "../../node_modules/rimraf/bin.js",
    "symlink": "node ../../scripts/symlink.js"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "circular-json": "^0.5.9",
    "dot-prop-immutable": "^1.5.0",
    "flat": "^4.1.0",
    "humanize-duration": "^3.15.3",
    "lodash": "^4.17.11",
    "minimist": "^1.2.3",
    "moment": "^2.22.2",
    "papaparse": "^5.2.0",
    "performance-now": "^2.1.0",
    "pluralize": "^8.0.0",
    "reselect": "^4.0.0",
    "thenby": "^1.3.0"
  }
}
